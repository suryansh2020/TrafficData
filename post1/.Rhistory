testMphDist <- function(time,distance, speed){
  count <- 1
  ans <- c()
  while(count < length(speed)){
    ans <- c(ans,
             mphDist(mph(time[count],
                         distance[count]),
                     speed[count]))
    
    
    count = count + 1
  }
  return(ans)
}
